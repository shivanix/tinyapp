<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <% if(typeof user === "undefined"){ %>
          <a class="nav-item nav-link" href="/login">Create New URL</a>
          <% } else{ %>
            <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
          
          <% } %>
      </div>
    </div>
    <div>
      <% if(typeof user === "undefined" && typeof usercookie === "undefined"){ %>
        <form action="/login" method="GET">
        <input type="submit" value="Login">
        </form>
      
          <form action="/register" method="GET">
            <input type="submit" value="Register">
          </form>

        <% } else if(typeof user !== "undefined"){ %>
          <form action="/logout" method="POST">
            <label for="name"><h4><%= `Logged in as: ${user.email}` %></h4></label>
            <input type="submit" value="Logout">
          </form>
        
        <% }else if(usercookie !== "undefined"){ %>
          <form action="/logout" method="POST">
            <label for="name"><h4><%= `Logged in as: ${usercookie.email}` %></h4></label>
            <input type="submit" value="Logout">
          </form>
          <% } %>
   

    </div>
  </nav>
</header>